# First solution

print("Welcome to the Band Name Generator!")

print("What is the city you grew up in?")

born_city = input()

print("What is the name of your pet?")

pet_name = input()

print("Your Band Name could be: " + " " + born_city + " " + pet_name)

# Second Solution

print("Welcome to the Band Name Generator!")

born_city = input("What is the city you grew up in?\n")

pet_name = input("What is the name of your pet?\n")

print("Your Band Name could be: " + born_city + " " + pet_name)
